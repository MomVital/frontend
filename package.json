{
  "name": "momvital",
  "license": "0BSD",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --tunnel",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "postinstall": "node ./postinstall.js",
    "clean": "rm -rf node_modules && npm install && npx expo start -c",
    "reset": "rm -rf node_modules && rm -rf .expo && npm cache clean --force && npm install && npx expo start -c",
    "fix-nativewind": "npm uninstall nativewind tailwindcss && npm install nativewind@4.0.1 tailwindcss@3.3.2",
    "fix-assets": "node ./fix-assets.js",
    "fix-deps": "expo install react-native-safe-area-context@4.8.2 react-native-gesture-handler@~2.14.0",
    "fix-ios": "node ./fix-ios.js && npm run reset",
    "fix-gesture": "node ./fix-gesture-handler.js && npm run reset"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-community/datetimepicker": "7.6.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@types/socket.io-client": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "expo": "^52.0.37",
    "expo-av": "~15.0.2",
    "expo-camera": "~16.0.17",
    "expo-file-system": "~18.0.11",
    "expo-image": "~2.0.6",
    "expo-linear-gradient": "~12.7.2",
    "expo-secure-store": "~12.8.1",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "~0.27.2",
    "expo-video-thumbnails": "~9.0.3",
    "nativewind": "^4.0.1",
    "postcss": "^8.4.23",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "^0.76.7",
    "react-native-chart-kit": "^6.12.0",
    "react-native-reanimated": "~3.16.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "15.0.0",
    "react-native-web": "~0.19.13",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "typescript": "~5.3.3",
    "react-native-gesture-handler": "~2.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.73.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  },
  "private": true
}
